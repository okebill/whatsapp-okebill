{
  "name": "wabot-api",
  "version": "1.0.0",
  "description": "WhatsApp Bot API for integration with external services",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node test-api.js",
    "generate-api-key": "node utils/generateApiKey.js",
    "init-sessions": "node utils/initializeSessions.js",
    "setup-database": "node utils/setupDatabase.js",
    "fix-collation": "node utils/fixCollation.js",
    "setup": "npm run setup-database && npm run init-sessions",
    "postinstall": "npm run init-sessions"
  },
  "keywords": [
    "whatsapp",
    "api",
    "bot",
    "automation"
  ],
  "author": "okebill",
  "license": "MIT",
  "dependencies": {
    "@whiskeysockets/baileys": "^6.7.18",
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "mysql2": "^2.3.3",
    "pino": "^8.16.2",
    "qrcode": "^1.5.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.19"
  }
}
